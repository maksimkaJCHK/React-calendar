{
  "name": "react-production-calendar",
  "version": "0.0.6",
  "description": "React production calendar on React.js",
  "main": "lib/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "buildextract": "cross-env NODE_ENV=production webpack --config config/webpack.prod.extract.js",
    "compile": "npm run compile-js && npm run compile-styles",
    "compile-js": "babel src/calendar -d lib --copy-files && npm run minifier",
    "compile-styles": "node-sass --output-style compressed ./src/calendar/styles/ -o ./lib/styles/ ",
    "minifier": "terser ./lib/index.js -o ./lib/index.js -c -m && terser ./lib/components/calendarBody.js -o ./lib/components/calendarBody.js -c -m && terser ./lib/components/calendarDay.js -o ./lib/components/calendarDay.js -c -m && terser ./lib/components/calendarHead.js -o ./lib/components/calendarHead.js -c -m && terser ./lib/services/days.js -o ./lib/services/days.js -c -m && terser ./lib/services/month.js -o ./lib/services/month.js -c -m && terser ./lib/services/monthClass.js -o ./lib/services/monthClass.js -c -m && terser ./lib/services/time.js -o ./lib/services/time.js -c -m",
    "test": "jest"
  },
  "author": "Maksim Zhuchkov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maksimkaJCHK/react-production-calendar"
  },
  "keywords": [
    "react",
    "reactjs",
    "calendar",
    "production calendar",
    "react calendar",
    "react production calendar"
  ],
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.12.10",
    "autoprefixer": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "chunks-webpack-plugin": "^7.0.0",
    "core-js": "^3.8.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "jest-enzyme": "^7.1.2",
    "mini-css-extract-plugin": "^1.3.3",
    "node-sass": "^5.0.0",
    "raw-loader": "^4.0.2",
    "react-test-renderer": "^17.0.1",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "sugarss": "^3.0.3",
    "terser-webpack-plugin": "^5.0.3",
    "twig-html-loader": "^0.1.8",
    "url-loader": "^4.1.1",
    "webpack": "^5.10.3",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "dependencies": {}
}
